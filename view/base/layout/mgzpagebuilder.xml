<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Cytracon_PageBuilder::css/builder.css" media="all"/>
        <css src="Cytracon_PageBuilder::css/styles.css" media="all"/>
    </head>
    <body>
        <referenceBlock name="before.body.end">
            <block class="Magento\Framework\View\Element\Template" name="cytraconpagebuilder.js" template="Cytracon_PageBuilder::scripts.phtml"/>
        </referenceBlock>
        <referenceContainer name="js">
            <block class="Magento\Framework\View\Element\Template" template="Cytracon_PageBuilder::wysiwyg/js.phtml" name="cytracon.pagebuilder.wysiwyg.js"/>
        </referenceContainer>
    </body>
</page>